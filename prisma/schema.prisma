// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// schema.prisma

model Year {
  day    Int
  month  Int
  value  Decimal  @db.Decimal(10, 2)
  
  @@id([day, month])
}

model Day {
  day    Int
  month  Int
  hour   Int
  value  Decimal  @db.Decimal(10, 2)
  
  @@id([day, month, hour])
}
